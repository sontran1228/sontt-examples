<html lang="en">
<head>
   <meta charset="utf-8" />
   <title>jQuery UI Tabs - Default functionality</title>
   <script>
      $(function() {
         $("#TodoApplication").tabs();
         $("li>a, label").button();
      });
   </script>
</head>
<body> 
   <div id="TodoApplication">
      <ul>
         <li><a href="#ListTodos">My List</a></li>    
      </ul>
      <p id="NewTodo">
         <select id="NewPriorityInput" class="Priority Border-Background">
            <option value="0">None</option>
            <option value="1">High</option>
            <option value="2">Med</option>
            <option value="3">Low</option>
         </select>
         <input id="NewTodoInput" class="Border-Background" onkeypress="if(event.keyCode == 13) todoApp.addTodo(this);" placeholder="What needs to be done?"/>
         <a href="javascript:void(0)" onclick="todoApp.addTodo(this);" class="tag-a-css AddTodoBtn">Add Todo</a>
      </p>
      <p class="TodoFunctions">
         <a href="javascript:void(0)" onclick="todoApp.completeTodo(this);" id="CompleteAll">Complete All</a>&nbsp;
         <a href="javascript:void(0)" onclick="todoApp.completeTodo(this);" id="IncompleteAll">Incomplete All</a>&nbsp;
         <a href="javascript:void(0)" onclick="todoApp.removeTodo(this);" id="DeleteComplete">Delete Completed</a>&nbsp;
         <a href="javascript:void(0)" onclick="todoApp.removeTodo(this);" id="DeleteAll">Delete All</a>
      </p>
      <div id="ListTodos">
      </div>
   </div>
</body>
</html>